{
    "parser": "babel-eslint",
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "env": {
        "node": "true",
        "browser": "true",
        "es6": "true",
        "jest": "true"
    },
    "rules": {
        "no-console": 1,
        "no-debugger": 1,
        "no-unused-vars": 1,
        "react/jsx-uses-react": 2
    }
}
